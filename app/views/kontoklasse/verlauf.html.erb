<h2>Übersicht von Kontoklasse: <%= @kkl %></h2>

<%= link_to "zurück", "/kontoklasse" %>

<table style="margin: 50px auto">
  <tr>
    <td>Konto-Nr.</td>
    <td>AbDatum</td>
  </tr>
  
  <% @kontenklassenverlauf.each do |konto| %>
  
    <tr>
      <td><%= konto.ktoNr %></td>
      <td><%= konto.kklAbDatum %></td>
    </tr>
  
  <% end %>
</table>
