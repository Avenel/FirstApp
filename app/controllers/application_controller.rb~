class ApplicationController < ActionController::Base
   protect_from_forgery
  
   def test
      
      @person = Person.new
      person.rolle = :G
      person.name = "Mustermann"
      person.vorname = "Max"
      person.save


      tel = Telefon.new
      tel.pnr = person.pnr
      tel.lfdNr = 1
      tel.telefonNr = "020122"
      tel.telefonTyp = "handy"
      tel.save
      
   end
  
end
